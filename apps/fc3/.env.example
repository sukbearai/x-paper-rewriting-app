# Shared secret used to verify Supabase webhook signatures
SUPABASE_HOOK_SECRET=

# Tencent Cloud SMS application ID issued by the console
TENCENT_SMS_SDK_APP_ID=
# Tencent Cloud SMS signature name bound to the application
TENCENT_SMS_SIGN=
# Tencent Cloud SMS template ID for the OTP message
TENCENT_SMS_TEMPLATE_ID=

# Tencent Cloud API secret ID (from CAM credentials)
SECRET_ID=
# Tencent Cloud API secret key (from CAM credentials)
SECRET_KEY=

# Optional: override SMS region (defaults to ap-guangzhou)
TENCENT_SMS_REGION=
# Optional: session token when using temporary credentials
TENCENT_SMS_TOKEN=
# Optional: response language, e.g. en-US or zh-CN
TENCENT_SMS_LANGUAGE=