# Copy this file to .env and populate each value before running the server.

# Supabase project base URL, for example https://project-id.supabase.co
SUPABASE_URL=

# Supabase anon/public API key used for server-side Supabase client calls
SUPABASE_ANON_KEY=

# Supabase send-sms webhook secret (v1,whsec_... from the Supabase dashboard)
SUPABASE_HOOK_SECRET=

# Tencent Cloud SMS application ID (SmsSdkAppId in the console)
TENCENT_SMS_SDK_APP_ID=

# Tencent Cloud SMS signature name (SignName configured for the template)
TENCENT_SMS_SIGN=

# Tencent Cloud SMS verification template ID (TemplateId containing the OTP slot)
TENCENT_SMS_TEMPLATE_ID=

# Tencent Cloud API SecretId used to sign TC3 requests
SECRET_ID=

# Tencent Cloud API SecretKey paired with the SecretId
SECRET_KEY=