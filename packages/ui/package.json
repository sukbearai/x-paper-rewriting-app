{
  "name": "@suwujs/ui",
  "type": "module",
  "version": "0.102.1",
  "description": "vue3 component library",
  "author": "suk.bear <suk.bear.suwu@gamil.com>",
  "license": "MIT",
  "homepage": "https://github.com/sukbearai/x-dev-tools#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sukbearai/x-dev-tools"
  },
  "bugs": "https://github.com/sukbearai/x-dev-tools/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./ui.css": "./dist/index.css",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build -w & pnpm uno-watch",
    "build": "vite build && pnpm uno",
    "uno": "unocss -m",
    "uno-watch": "unocss -w"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@suwujs/preset": "workspace:*",
    "@suwujs/utils": "workspace:*",
    "element-plus": "catalog:",
    "unplugin-vue-components": "^0.27.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "unplugin-element-plus": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  }
}
